<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h1>Todos</h1>

    <ul>
      <% todos.forEach(function(todo) { %>
        <li>
          <%= todo.todo %>
          -
          <%= todo.done ? 'done' : 'not done' %>
        </li>
      <% }); %>
    </ul>
  </body>
</html>
